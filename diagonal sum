class Solution:
    def DiagonalSum(self, a):
        s=0
        for i in range(len(a)):
            for j in range(len(a)):
                if i==j:
                    s+=a[i][j]
        for i in range(len(a)):
            for j in range(len(a)):
                if i+j==len(a)-1:
                     s+=a[i][j]
        return s
